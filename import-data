    },
    "maxCapacity": {"type": "Property", "value": 100},
    "name": {"type": "Property", "value": "Wall Unit 1"},
    "numberOfItems": {"type": "Property", "value": 15},
    "stocks": {
      "type": "Relationship", "object": "urn:ngsi-ld:Product:003"
